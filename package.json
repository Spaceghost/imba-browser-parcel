{
  "name": "imba-browser-parcel",
  "version": "1.0.0",
  "description": "",
  "main": "src",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "imbac --output src/scripts --target web imba && parcel build src/index.html --out-dir release",
    "devserv": "parcel src/index.html --out-dir .cache",
    "imba": "imbac --watch --output src/scripts --target web imba",
    "dev": "npm-run-all --parallel imba devserv"
  },
